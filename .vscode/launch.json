  "version": "0.2.0",
  "configurations": [
    {
      "type": "lldb",
      "request": "launch",
      "name": "Debug unit tests in library 'fs_cli'",
      "cargo": {
        "args": [
          "test",
          "--no-run",
          "--lib",
          "--package=fs_cli"
        ],
        "filter": {
          "name": "fs_cli",
          "kind": "lib"
        }
      },
      "args": [],
      "cwd": "${workspaceFolder}"
    },
    {
      "type": "lldb",
      "request": "launch",
      "name": "Debug executable 'fs_cli'",
      "cargo": {
        "args": [
          "build",
          "--bin=fs_cli",
          "--package=fs_cli"
        ],
        "filter": {
          "name": "fs_cli",
          "kind": "bin"
        }
      },
      "args": [
        "--start=./ruby_src/test.rb"
      ],
      "cwd": "${workspaceFolder}"
    },
    {
      "type": "lldb",
      "request": "launch",
      "name": "Debug unit tests in executable 'fs_cli'",
      "cargo": {
        "args": [
          "test",
          "--no-run",
          "--bin=fs_cli",
          "--package=fs_cli"
        ],
        "filter": {
          "name": "fs_cli",
          "kind": "bin"
        }
      },
      "args": [],
      "cwd": "${workspaceFolder}"
    },
    {
      "type": "lldb",
      "request": "launch",
      "name": "sample binary",
      "program": "${workspaceFolder}/bin",
    },
    {
      "type": "lldb",
      "request": "launch",
      "name": "sample cli",
      "program": "${workspaceFolder}/target/debug/fs_cli",
      "args": [
        "/workspaces/ruby_packager",
        "ruby_src/",
        "/usr/local/rvm/rubies/ruby-3.2.2/lib/ruby/site_ruby/3.2.0",
        "--start=./ruby_src/test.rb"
      ]
    }
  ]
}
